{
  "name": "bikomat",
  "version": "5.2.0",
  "license": "MIT",
  "scripts": {
    "build-test": "ng test --single-run",
    "build": "ng build",
    "build-app": "ng build app --prod",
    "build-loan": "ng build --prod --app loan",
    "build-dashboard": "ng build --prod --app dashboard",
    "build-register": "ng build --prod --app register",
    "build-login": "ng build --prod --app finai.login",
    "start": "webpack-dev-server --port=4200",
    "test": "ng test",
    "serve": "ng serve --app app",
    "serve-credit": "ng serve --app loan --port=4200",
    "serve-dashboard": "ng serve --app dashboard --port=4200",
    "serve-login": "ng serve --app finai.login --port=4200",
    "serve-register": "ng serve --app register --port=5200",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js",
    "sprites-register": "svg-sprite --config sprite-config.json src/assets/svg/lib/*.svg && ng serve --port=4200 --app register",
    "css-register": "ng serve --port=80  --sourcemap --extractCss --app register",
    "sprites-app": "svg-sprite --config sprite-config.json src/assets/svg/lib/*.svg && ng serve --port=4200 --app app",
    "sprites-app-prod": "svg-sprite --config sprite-config.json src/assets/svg/lib/*.svg && ng serve --port=4200 --app app --prod",
    "css-app": "ng serve --port=80  --sourcemap --extractCss --app app",
    "debug-register": "ng serve --env=prod --sourcemap --app register",
    "debug-app": "ng serve --env=prod --sourcemap --app app",
    "sprites-dashboard": "svg-sprite --config sprite-config.json src/assets/svg/lib/*.svg && ng serve --port=4200 --app dashboard",
    "css-dashboard": "ng serve --port=80  --sourcemap --extractCss --app dashboard",
    "debug-dashboard": "ng serve --port=80 --env=prod --sourcemap --app dashboard",
    "sprites-loan": "svg-sprite --config sprite-config.json src/assets/svg/lib/*.svg && ng serve --port=4200 --app loan",
    "css-loan": "ng serve --port=80  --sourcemap --extractCss --app loan",
    "debug-loan": "ng serve --port=80 --env=prod --sourcemap --app loan",
    "sprites-login": "svg-sprite --config sprite-config.json src/assets/svg/lib/*.svg && ng serve --port=4200 --app finai.login",
    "css-login": "ng serve --port=80  --sourcemap --extractCss --app finai.login",
    "debug-login": "ng serve --port=80 --env=prod --sourcemap --app finai.login",
    "docs": "node_modules/.bin/compodoc -p tsconfig.json -d docs",
    "serve-docs": "node_modules/.bin/compodoc -s -d docs",
    "svgoptim": "svgo -f src/assets/img",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "lint": "sass-lint -v",
    "lint:fix": "sass-lint-auto-fix -c .sass-lint-auto-fix.yml"
  },
  "private": true,
